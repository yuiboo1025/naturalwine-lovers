<div class='container my-4'>
  
  <%= render 'admin/wines/member_info', member: @member %>
  
  <div class="row">
    <h5 class="col text-center mx-auto",  style="color:#3d83bf;">
      <%= @member.name %>のまた飲みたいナチュール🍷<span> (<%= @again_wines.count %>件)</span>
    </h5>
  </div>
  <div class='row'>
    <div class='col'>

      <%= render 'admin/wines/index', wines: @again_wines, member: @member, genres: @genres %>

      <div class="d-flex justify-content-center mt-5">
        <%#= paginate @wines %>
      </div>

    </div>
  </div>
</div>